<section class="library">
<header class="lo-full visuallyhidden">
  <h1>Biblioteket</h1>
</header>
<section class="lo-full">
<nav class="navbar navbar-default">
  <div class="container-fluid">
     <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#library-navbar-collapse" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    </div>
    <div class="collapse navbar-collapse" id="library-navbar-collapse">
      {{#each library}}
      <ul class="nav navbar-nav">
        {{#if elibrary}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">E-biblioteket <span class="caret"></span></a>

          <ul class="dropdown-menu">
          {{#each elibrary}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
        {{#if howtowrite}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Skrive & refere <span class="caret"></span></a>
          <ul class="dropdown-menu">
          {{#each howtowrite}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
        {{#if researchandpublication}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Forskning & publisering <span class="caret"></span></a>
          <ul class="dropdown-menu">
          {{#each researchandpublication}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
        {{#if orderandrenew}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Låne & bestille <span class="caret"></span></a>
          <ul class="dropdown-menu">
          {{#each orderandrenew}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
        {{#if helpservices}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="visuallyhidden">Hjelp fra biblioteket</span> Tjenester <span class="caret"></span></a>
          <ul class="dropdown-menu">
          {{#each helpservices}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
        {{#if about}}
          <li><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Om biblioteket <span class="caret"></span></a>
          <ul class="dropdown-menu">
          {{#each about}}
            <li><a href="#/biblioteket/side/{{id}}">{{{pagetitle}}}</a></li>
          {{/each}}
          </ul></li>
        {{/if}}
      </ul>

      {{/each}}

    </div>

</div>
</nav>
</section>
<aside class="lo-auron-1-3 library-basic-info float-right">
<section>
  <h3>Sosiale medier</h3>

<div class="btn-group btn-group-justified" role="group" aria-label="Sosiale medier">
  <a role="button" class="btn btn-default" href="https://www.facebook.com/bibhiof">Facebook</a>
  <!--<a role="button" class="btn btn-default" href="https://www.facebook.com/bibhiof">Twitter</a>-->
  <a role="button" class="btn btn-default" href="http://instagram.com/hiofbibfred">Instagram Fredrikstad</a>
  <a role="button" class="btn btn-default" href="http://instagram.com/hiofbibhalden">Instagram Halden</a>
  <a role="button" class="btn btn-default" href="https://www.youtube.com/channel/UCfDe1CghuDZt0J7hxMvjrJQ">Youtube</a>

</div>


</section>
<section>
  {{!-- Åpningstider --}}
  <p>Biblioteket er åpent hele sommeren <br><strong>8:00 - 15:00 mandag - fredag</strong></p></section>
</aside>

<section class="lo-auron-2-3 library-search">
  {{!-- Søk --}}
  <ul class="nav nav-tabs nav-justified" role="tablist">
    <li role="presentation" class="active"><a href="#oria" aria-controls="oria" role="tab" data-toggle="tab">Mitt bibliotek</a></li>
    <li role="presentation"><a href="#bibsys" aria-controls="bibsys" role="tab" data-toggle="tab">Alle bibliotek</a></li>
    <li role="presentation"><a href="#googlescholar" aria-controls="googlescholar" role="tab" data-toggle="tab">Google Scholar</a></li>

  </ul>
  <div class="tab-content">
  <div role="tabpanel" class="tab-pane fade in active" id="oria">


  <form class="form-inline" action="http://www.oria.no/" enctype="application/x-www-form-urlencoded; charset=utf-8" method="post" name="searchForm" target="_self">
    <div class="input-group">

      <label class="sr-only" for="library-search">Søk i Oria</label>
      <input type="email" class="form-control " id="library-search" placeholder="Søk etter bøker, artikler, tidsskrifter m.m.">

      <span class="input-group-btn">
        <button type="submit" class="btn btn-primary ">Søk</button>
      </span>
    </div>
  </form>
  <p><a href="http://bibsys-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/search.do?mode=Advanced&ct=AdvancedSearch&dscnt=0&dstmp=1409567179941&vid=HIO">Avansert søk</a></p>
    </div>

  <div role="tabpanel" class="tab-pane" id="bibsys">


    <form class="form-inline" action="http://www.oria.no/" enctype="application/x-www-form-urlencoded; charset=utf-8" method="post" name="searchForm" target="_self">
    <div class="input-group">

        <label class="sr-only" for="library-search">Søk i bibsys</label>
        <input type="email" class="form-control " id="library-search" placeholder="Søk i bibsys">

      <span class="input-group-btn">
        <button type="submit" class="btn btn-primary ">Søk</button>
        </span>
      </div>
    </form>
    </div>
  <div role="tabpanel" class="tab-pane" id="googlescholar">



    <form class="form-inline" action="http://www.oria.no/" enctype="application/x-www-form-urlencoded; charset=utf-8" method="post" name="searchForm" target="_self">
      <div class="input-group">
        <label class="sr-only" for="library-search">Søk i google scholar</label>
        <input type="email" class="form-control " id="library-search" placeholder="Søk i Google scholar">
        <span class="input-group-btn">
      <button type="submit" class="btn btn-primary ">Søk</button>
      </span>
      </div>
    </form>
  </div>

</section>







<section class="lo-full padding-off library-news">
  <header class="lo-full">
    <h1 class="float-left">Aktuelt fra biblioteket</h1>
    <a class="btn btn-link arrow arrow-right float-right" href="#/biblioteket/aktuelt">Flere artikler</a>
  </header>
